{
  "scopeName": "source.asc",
  "patterns": [
    {
      "match": ";.*",
      "name": "comment.line.semicolon.asc"
    },
    {
      "match": "\\b(IF|THEN|ELSE|DO|END|REG|SEND|MEAS|START|WAIT|GOSUB)\\b",
      "name": "keyword.control.asc"
    },
    {
      "match": "\\b(int|float|string|bool)\\b",
      "name": "storage.type.asc"
    },
    {
      "match": "\\b(TRUE|FALSE|ON|OFF)\\b",
      "name": "constant.language.asc"
    },
    {
      "name": "constant.numeric.hexadecimal.asc",
      "match": "#[0-9A-Fa-f]+"
    },
    {
      "name": "constant.numeric.logical.asc",
      "match": "\\.[0-9]+"
    },
    {
      "match": "\\b(0[xX][0-9a-fA-F]+|\\d+(\\.\\d+)?([eE][+-]?\\d+)?|0[0-7]+)\\b",
      "name": "constant.numeric.asc"
    },
    {
      "match": "\\\".*?\\\"",
      "name": "string.quoted.double.asc"
    },
    {
      "match": "\\\".*?\\\"",
      "name": "string.quoted.double.asc"
    }
  ],
  "repository": {},
  "scopeName": "source.asc"
}